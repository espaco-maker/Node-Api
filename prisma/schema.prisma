datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Messages {
  id        Int      @id @default(autoincrement())
  FirstName String
  LastName  String
  Email     String
  Message   String
  creat_at  DateTime @default(now())

  Users Users[]
  @@map("messages")
}

model Userssignup {
  id        Int      @id @default(autoincrement())
  Email     String
  FirstName String
  LastName  String
  Password  String
  creat_at  DateTime @default(now())

  @@map("userssignup")
}

model Users {
  id         Int      @id @default(autoincrement())
  Email      String
  FirstName  String
  LastName   String
  Messages   Messages @relation(fields: [messagesId], references: [id])
  creat_at   DateTime @default(now())
  messagesId Int

  @@map("users")
}
